{
  "name": "@xchange-ecosystem/xchange-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "concurrently 'npm:watch:*'",
    "watch:storybook": "ng run ecosystem-web:storybook",
    "watch:tailwind": "npx tailwindcss -i ./src/assets/css/styles.scss -o ./projects/ngx-xchange-ui/src/assets/css/base.css --watch",
    "build-storybook": "ng run ecosystem-web:build-storybook",
    "build:lib": "npx tailwindcss -i ./src/assets/css/styles.scss -o ./projects/ngx-xchange-ui/src/assets/css/base.css && ng build ngx-xchange-ui",
    "perbuild:icons": "node ./projects/ngx-xchange-ui/prebuild.js",
    "build:icons": "ng build ngx-xchange-icons",
    "build:lib:watch": "ng build ngx-xchange-ui --watch"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/common": "^17.3.8",
    "@angular/compiler": "^17.3.8",
    "@angular/core": "^17.3.8",
    "@angular/forms": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/router": "^17.3.8",
    "rxjs": "~7.8.0",
    "sass": "^1.83.4",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "~17.3.7",
    "@angular/compiler-cli": "^17.3.8",
    "@compodoc/compodoc": "^1.1.16",
    "@storybook/addon-docs": "8.5.1",
    "@storybook/addon-essentials": "8.5.1",
    "@storybook/addon-interactions": "8.5.1",
    "@storybook/addon-links": "8.5.1",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/angular": "8.5.1",
    "@storybook/blocks": "8.5.1",
    "@storybook/testing-library": "^0.2.1",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "handlebars": "4.7.8",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "postcss": "^8.4.30",
    "storybook": "8.5.1",
    "tailwindcss": "^3.3.3",
    "typescript": "5.4.5",
    "zone.js": "0.14.5"
